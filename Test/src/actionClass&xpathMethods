public actionClass&xpathMethods{

public stattic void main(String args[]){

Actions action= new Actions(driver);
action.moveToElement(driver.findElement(By.id("test"))).build().perform();
action.clickAndHold(driver.findElement(By.xpath("//*[@id='test']"))).moveToElement(driver.findElement(By.id("sumit"))).release().build().perform();
driver.findElement(By.xpath("//input[@class='test']"));
driver.findElement(By.xpath("//input[contains(@class,'test')]")).sendKeys("test");
driver.findElement(By.xpath("//input[starts-with(@id,'test_')]")).click();
driver.findElement(By.xpath("//input[ends-with(@in,test')]")).sendKeys("test");
}
}
